algoritmo "Somando Valores"
var

   cont, n, s: inteiro

inicio

      cont <- 1
      s <- 0
      enquanto (cont<=5) faca
      escreva ("Escolha seu", cont, " Numero: ")
      leia (n)
      s <- s + n
      cont <- cont + 1
      fimenquanto
      escreva ("A soma desses valores é: ", s)
      
fimalgoritmo